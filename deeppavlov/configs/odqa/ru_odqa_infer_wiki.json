{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "best_answer"
    ],
    "pipe": [
      {
        "config_path": "../deeppavlov/configs/ranking/ru_ranker_tfidf_wiki.json",
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids"
        ]
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw"
        ],
        "data_dir": "odqa",
        "join_docs": false,
        "shuffle": false,
        "data_url": "http://files.deeppavlov.ai/datasets/wikipedia/ruwiki.db"
      },
      {
        "name": "document_chunker",
        "in": ["context_raw"],
        "out": ["chunks"],
        "flatten_result": true
      },
      {
        "name": "string_multiplier",
        "in": ["question_raw", "chunks"],
        "out":["questions"]
      },
      {
        "name": "logit_ranker",
        "squad_model_config": "../deeppavlov/configs/squad/squad_ru.json",
        "in": [
          "chunks",
          "questions"
        ],
        "out": [
          "best_answer"
        ]
      }
    ]
  },
  "metadata": {
    "requirements": [
      "../dp_requirements/tf-gpu.txt"
    ],
    "labels": {
      "server_utils": "ODQA"
    },
    "download": [
      "http://files.deeppavlov.ai/deeppavlov_data/squad_model_ru_1.3_cpu_compatible.tar.gz",
      "http://files.deeppavlov.ai/datasets/wikipedia/ruwiki.tar.gz",
      "http://files.deeppavlov.ai/deeppavlov_data/ru_odqa.tar.gz",
      {
        "url": "http://files.deeppavlov.ai/embeddings/ft_native_300_ru_wiki_lenta_nltk_word_tokenize/ft_native_300_ru_wiki_lenta_nltk_word_tokenize.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://files.deeppavlov.ai/embeddings/ft_native_300_ru_wiki_lenta_nltk_word_tokenize-char.vec",
        "subdir": "embeddings"
      }
    ]
  }
}